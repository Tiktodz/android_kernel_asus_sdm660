/ {
cpus {
	cpu@0 {
		efficiency = <618>;
		capacity-dmips-mhz = <618>;
	};

	cpu@4 {
		efficiency = <1024>;
		capacity-dmips-mhz = <1024>;
	};
};

energy_costs: energy-costs {
	compatible = "sched-energy";

	CPU_COST_0: core-cost0 {
		busy-cost-data = <
			  633600   43
			  902400   62
			 1113600   80
			 1401600  123
			 1536000  144
			 1612800  160
		>;
		idle-cost-data = <
			3 2 1
		>;
	};

	CPU_COST_1: core-cost1 {
		busy-cost-data = <
			 1113600  232
			 1401600  326
			 1747200  488
			 1804800  510
		>;
		idle-cost-data = <
			3 2 1
		>;
	};
};

	reserved-memory {
	ramoops {
		status = "ok";
		compatible = "removed-dma-pool", "ramoops";
		no-map;
		reg = <0x0 0xA0000000 0x0 0x00400000>;
		console-size = <0x200000>;
		pmsg-size = <0x200000>;
		};

		cont_splash_mem: splash_region@9d400000 {
			reg = <0x0 0x9d400000 0x0 0x02400000>;
		};

		/delete-node/ dfps_data_mem@0x9f7ff000;

		dfps_data_mem: dfps_data_mem@0x9f800000 {
			reg = <0x0 0x9f800000 0x0 0x00001000>;
			label = "dfps_data_mem";
		};
	};
};

&firmware {
	android {
		vbmeta {
			status = "disabled";
		};

		fstab {
			vendor {
				fsmgr_flags = "wait";
				status = "ok";
			};

			system {
				compatible = "android,system";
				dev = "/dev/block/platform/soc/c0c4000.sdhci/by-name/system";
				type = "ext4";
				mnt_flags = "ro,barrier=1,discard";
				fsmgr_flags = "wait";
				status = "ok";
			};

			system_ext {
				compatible = "android,system_ext";
				dev = "/dev/block/platform/soc/c0c4000.sdhci/by-name/system_ext";
				type = "ext4";
				mnt_flags = "ro,barrier=1,discard";
				fsmgr_flags = "wait";
				status = "ok";
			};

			product {
				compatible = "android,product";
				dev = "/dev/block/platform/soc/c0c4000.sdhci/by-name/product";
				type = "ext4";
				mnt_flags = "ro,barrier=1,discard";
				fsmgr_flags = "wait";
				status = "ok";
			};
		};
	};
};

&wdog {
	qcom,bark-time = <20000>;
};

&msm_gpu {
	/* sdm636.dtsi defaults to A509, but X00T has A512 tweaks */
	qcom,chipid = <0x05010200>;
};

&mem_client_3_size {
		/delete-property/ qcom,allocate-on-request;

		qcom,allocate-boot-time;
};

&ufs1 {
	/delete-property/ pinctrl-names;
	/delete-property/ pinctrl-0;
	/delete-property/ pinctrl-1;
};

&soc {
	gpio_keys {
		/delete-node/ camera_focus;
		/delete-node/ camera_snapshot;

		vol_up {
			gpios = <&tlmm 44 0x1>;
		};

		vol_down {
			label = "volume_down";
			gpios = <&tlmm 43 0x1>;
			linux,input-type = <1>;
			linux,code = <114>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};
};

&thermal_zones {
	/delete-node/ vbat_adc;
	/delete-node/ vbat_low;
	/delete-node/ vbat_too_low;
	qcom,msm_fastrpc {
		/delete-property/ qcom,fastrpc-adsp-sensors-pdr;
	};
};
